<h3 class="mt-5"><%= title %></h3>
  <div class="books-container mb-5">
    <% filteredBooks.forEach(user => { %>
      <div class="cards-container">
       <div class="wrapper">
         <a href="/detail/<%= user.id %>">
          <img src="<%= user.image %>" alt="<%= user.name %>"/>
         </a>
          <div class="details">
            <h2><%= user.name %></h2>
            <p><%= user.author %></p>
         </div>
        </div>
      </div>
     <% }) %>
   </div>
