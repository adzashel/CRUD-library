<h3>New Release</h3>
<div class="item-container">
<% newBooks.forEach(item => { %>
  <div class="item-header">
   <a href="/new-books-detail/<%= item.id %>">
    <img
    src="<%= item.image %>"
    alt="retorika"
  />
   </a>
    <div class="item-detail">
      <h2><%= item.name %></h2>
      <p><%= item.author %></p>
      <p class="desc"><%= item.description %></p>
    </div>
  </div>
<% }) %>

</div>
