   <div class="books-container">
    <% users.forEach(user => { %>
      <!-- show data user -->
      <div class="card" style="width: 14rem;">
        <img src="<%= user.image %>" class="card-img-top image" alt="...">
        <div class="card-body">
          <h5 class="card-title"> <%= user.name %></h5>
          <p class="card-text"><%= user.author %></p>
          <a href="#" class="btn btn-primary">Detail</a>
        </div>
      </div>
     <% }) %>
   </div>
